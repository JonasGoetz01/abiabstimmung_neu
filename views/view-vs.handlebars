<nav aria-label="breadcrumb">
    <ol class="breadcrumb">
        <li class="breadcrumb-item"><a href="/abiabstimmung/">Home</a></li>
        <li class="breadcrumb-item"><a href="/abiabstimmung/vsmanagement/">Versus Fragen verwalten</a></li>
        <li class="breadcrumb-item active" aria-current="page">Versus Frage anschauen</li>
    </ol>
</nav>

<div class="p-5">
    {{#each rows}}
        <div class="row mb-5">
            <div class="col text-center">
                <h3>Versus Frage</h3>
            </div>
        </div>
        <div class="row">
            <div class="col">
                <table class="table">
                    <thead>
                    <tr>
                        <th scope="col">ID</th>
                        <th scope="col">Option 1</th>
                        <th scope="col">Option 2</th>
                    </tr>
                    </thead>
                    <tbody>
                    <tr>
                        <th scope="row">{{this.ID}}</th>
                        <td>{{this.option1}}</td>
                        <td>{{this.option2}}</td>
                    </tr>
                    </tbody>
                </table>
            </div>
        </div>
    {{/each}}
</div>